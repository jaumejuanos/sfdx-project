<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Interest_Rate__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Principal__c &lt; 10000, 0.02, 
  IF(Principal__c &gt;= 10000 &amp;&amp;
